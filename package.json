{
  "name": "vite-vue",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.1",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "slint": "stylelint \"src/**/*.(vue|scss|css)\"",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@ngify/http": "^1.5.3",
    "@vueuse/components": "^10.4.1",
    "@vueuse/core": "^10.4.1",
    "@vueuse/integrations": "^10.4.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "sortablejs": "^1.15.0",
    "universal-cookie": "^6.1.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.42.0",
    "@iconify-json/tabler": "^1.1.91",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.6.1",
    "@unocss/eslint-config": "^0.55.7",
    "@unocss/preset-icons": "^0.55.7",
    "@unocss/reset": "^0.55.7",
    "@unocss/transformer-directives": "^0.55.7",
    "@vitejs/plugin-vue": "^4.3.4",
    "lint-staged": "^14.0.1",
    "rxjs": "^7.8.1",
    "sass": "^1.67.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "uuid": "^9.0.1",
    "vite": "^4.4.9",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-devtools": "1.0.0-rc.4",
    "vue-tsc": "^1.8.11"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix",
    "*.(vue|scss|css)": "stylelint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ],
    "rules": {
      "no-console": "off"
    },
    "ignorePatterns": [
      "dist",
      "node_modules",
      "*.scss",
      "*.css"
    ]
  }
}
